<root>
    <BehaviorTree>
    <Fallback name="root">
        <LaneChange name="lane_change"
            waypoints="{waypoints}"
            request="{request}"
            hdmap_utils="{hdmap_utils}"
            current_time="{current_time}"
            step_time="{step_time}"
            entity_status="{entity_status}"
            vehicle_parameters="{vehicle_parameters}"
            updated_status="{updated_status}"
            target_speed="{target_speed}"
            other_entity_status="{other_entity_status}"
            entity_type_list="{entity_type_list}"
            to_lanelet_id="{to_lanelet_id}"/>
        <AcquirePosition name="acquire_position"
            waypoints="{waypoints}"
            request="{request}"
            hdmap_utils="{hdmap_utils}"
            current_time="{current_time}"
            step_time="{step_time}"
            entity_status="{entity_status}"
            vehicle_parameters="{vehicle_parameters}"
            updated_status="{updated_status}"
            target_speed="{target_speed}"
            other_entity_status="{other_entity_status}"
            entity_type_list="{entity_type_list}"
            target_lanelet_pose="{target_lanelet_pose}"/>
        <Fallback name="follow_lane_sequence">
            <FollowLane name="follow_lane"
                waypoints="{waypoints}"
                request="{request}"
                hdmap_utils="{hdmap_utils}"
                current_time="{current_time}"
                step_time="{step_time}"
                entity_status="{entity_status}"
                vehicle_parameters="{vehicle_parameters}"
                updated_status="{updated_status}"
                target_speed="{target_speed}"
                other_entity_status="{other_entity_status}"
                entity_type_list="{entity_type_list}"/>
            <Fallback name="follow_lane_behavior_selector">
                <FollowFrontEntity name="follow_front_entity"
                    waypoints="{waypoints}"
                    request="{request}"
                    hdmap_utils="{hdmap_utils}"
                    current_time="{current_time}"
                    step_time="{step_time}"
                    entity_status="{entity_status}"
                    vehicle_parameters="{vehicle_parameters}"
                    updated_status="{updated_status}"
                    target_speed="{target_speed}"
                    other_entity_status="{other_entity_status}"
                    entity_type_list="{entity_type_list}"/>
                <StopAtStopLine name="stop_at_stop_line"
                    waypoints="{waypoints}"
                    request="{request}"
                    hdmap_utils="{hdmap_utils}"
                    current_time="{current_time}"
                    step_time="{step_time}"
                    entity_status="{entity_status}"
                    vehicle_parameters="{vehicle_parameters}"
                    updated_status="{updated_status}"
                    target_speed="{target_speed}"
                    other_entity_status="{other_entity_status}"
                    entity_type_list="{entity_type_list}"/>
                <StopAtCrossingEntity name="stop_at_crossing_entity"
                    waypoints="{waypoints}"
                    request="{request}"
                    hdmap_utils="{hdmap_utils}"
                    current_time="{current_time}"
                    step_time="{step_time}"
                    entity_status="{entity_status}"
                    vehicle_parameters="{vehicle_parameters}"
                    updated_status="{updated_status}"
                    target_speed="{target_speed}"
                    other_entity_status="{other_entity_status}"
                    entity_type_list="{entity_type_list}"/>
                <Yield name="yield"
                    waypoints="{waypoints}"
                    request="{request}"
                    hdmap_utils="{hdmap_utils}"
                    current_time="{current_time}"
                    step_time="{step_time}"
                    entity_status="{entity_status}"
                    vehicle_parameters="{vehicle_parameters}"
                    updated_status="{updated_status}"
                    target_speed="{target_speed}"
                    other_entity_status="{other_entity_status}"
                    entity_type_list="{entity_type_list}"/>
            </Fallback>
        </Fallback>
    </Fallback>
    </BehaviorTree>
</root>